#%RAML  1.0 DataType
type: array
items:
  type: object
  properties:
    departureDateTime:
      required: true
      type: string
      example: "2019-07-30T16:41:41"
    fromLocation:
      required: true
      type: string
      example: "BEN-SG"
    toLocation:
      required: true
      type: string
      example: "KLG-MY"
    availableSeats:
      required: true
      type: number
      example: 20